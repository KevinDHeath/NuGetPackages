<UserControl x:Class="TestWPFPackages.Views.Introduction"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:vm="clr-namespace:TestWPFPackages.Core.ViewModels"
			 mc:Ignorable="d"
			 d:DataContext="{d:DesignInstance Type=vm:TestViewModel}"
			 d:DesignHeight="250" d:DesignWidth="300" d:Background="White">
	<Grid HorizontalAlignment="Stretch" Margin="20">
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>
		<TextBlock Grid.Row="0" Text="Introduction" Style="{StaticResource appViewTitleStyle}"/>
		<StackPanel Grid.Row="1" Margin="0,10">
			<CheckBox Content="Allow Can Execute?" IsChecked="{Binding CanExecuteTest, Mode=TwoWay}"/>
		</StackPanel>
	</Grid>
</UserControl>
